<?xml version="1.0" encoding="UTF-8"?>
<syntax name="twee">
    <meta>
        <name>Twee</name>
        <type>markup</type>
        <preferred-file-extension>twee</preferred-file-extension>
    </meta>

    <detectors>
        <extension priority="1.0">tw,twee,twee3</extension>
    </detectors>

    <tree-sitter language="twee">
        <folds/>
        <highlights/>
        <injections/>
        <symbols/>
        <text-checking/>
    </tree-sitter>
</syntax>
